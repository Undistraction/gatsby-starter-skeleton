{
  "name": "gatsby-starter-skeleton",
  "description": "Gatsby Skeleton Starter",
  "version": "1.0.0",
  "author": "Pedr Browne <info@undistraction.com>",
  "keywords": [
    "gatsby",
    "styled-components",
    "cssapi"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop",
    "open": "open http://localhost:8000",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "preview": "gatsby build && gatsby serve",
    "test": "jest src/config src/styles --watch ",
    "graph": "open http://localhost:8000/___graphql",
    "lint:js": "eslint src",
    "lint:css": "stylelint './src/**/*.js'",
    "lint": "npm run lint:js && npm run lint:css",
    "demo": "yarn demo:import && yarn demo:copy && yarn demo:build && yarn demo:githubPages && yarn demo:open",
    "demo:import": "bash scripts/demo-import.sh",
    "demo:copy": "bash scripts/demo-copy-content.sh",
    "demo:build": "cd ./demo/project && gatsby build --prefix-paths",
    "demo:githubPages": "gh-pages -d demo/project/public",
    "demo:open": "open https://undistraction.github.io/gatsby-starter-skeleton/",
    "demo:update": "yarn demo:import && yarn demo:dev",
    "demo:deleteCache": "bash scripts/demo-delete-cache.sh",
    "demo:dev": "yarn demo:deleteCache && yarn demo:copy && cd demo/project && yarn dev",
    "demo:serve": "cd ./demo/project && gatsby serve",
    "surge:deploy": "surge --domain gatsby-starter-skeleton.surge.sh public/",
    "publish:patch": "npm version patch && sudo npm publish",
    "publish:minor": "npm version minor && sudo npm publish",
    "publish:major": "npm version major && sudo npm publish",
    "postpublish": "yarn demo"
  },
  "dependencies": {
    "cssapi": "0.9.7",
    "object-assign": "^4.1.1",
    "ramda": "^0.25.0",
    "ramda-adjunct": "^2.9.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "sharp": "^0.20.5",
    "styled-components": "^3.2.6",
    "styled-normalize": "^4.0.0",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-object-assign": "^7.0.0-beta.55",
    "babel-plugin-add-module-exports": "^0.3.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "fbjs": "^0.8.17",
    "gatsby": "^2.0.0-beta.61",
    "gatsby-image": "^2.0.0-beta.7",
    "gatsby-paginate": "^1.0.6",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-catch-links": "^2.0.2-beta.4",
    "gatsby-plugin-favicon": "^3.1.2",
    "gatsby-plugin-feed": "^2.0.0-beta.4",
    "gatsby-plugin-google-analytics": "^2.0.0-beta.3",
    "gatsby-plugin-node-fields": "^1.0.0",
    "gatsby-plugin-page-creator": "^2.0.0-beta.5",
    "gatsby-plugin-react-helmet": "^3.0.0-beta.4",
    "gatsby-plugin-remove-trailing-slashes": "^2.0.0-beta.3",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sharp": "^2.0.0-beta.7",
    "gatsby-plugin-sitemap": "^2.0.0-beta.3",
    "gatsby-plugin-styled-components": "^3.0.0-beta.3",
    "gatsby-plugin-twitter": "^2.0.0-beta.3",
    "gatsby-remark-code-repls": "^2.0.0-beta.4",
    "gatsby-remark-copy-linked-files": "^2.0.0-beta.3",
    "gatsby-remark-embed-snippet": "^3.0.0-beta.3",
    "gatsby-remark-embed-video": "^1.4.0",
    "gatsby-remark-embedded-codesandbox": "^1.2.0",
    "gatsby-remark-images": "^2.0.1-beta.9",
    "gatsby-remark-prismjs": "^3.0.0-beta.5",
    "gatsby-remark-responsive-iframe": "^2.0.0-beta.3",
    "gatsby-remark-responsive-image": "^1.0.0-beta.4",
    "gatsby-remark-smartypants": "^2.0.0-beta.3",
    "gatsby-source-filesystem": "^2.0.1-beta.10",
    "gatsby-transformer-remark": "^2.1.1-beta.5",
    "gatsby-transformer-sharp": "^2.1.1-beta.6",
    "gh-pages": "^1.1.0",
    "github-pages": "^3.0.2",
    "jest": "^21.2.1",
    "moment": "^2.22.2",
    "postcss-sass": "^0.2.0",
    "prettier": "^1.14.0",
    "prismjs": "^1.15.0",
    "rehype-react": "^3.0.2",
    "remark-retext": "^3.1.0",
    "retext-english": "^3.0.0",
    "slugify": "^1.2.1",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "surge": "^0.19.0",
    "webpack": "^4.16.3"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 2 versions"
  ]
}
